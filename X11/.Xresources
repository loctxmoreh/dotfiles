! --------------------------------------------------------------
! URxvrt standard Arco Linux theme
! --------------------------------------------------------------

! General settings
URxvt*saveLines:        50000
URxvt*termName:         rxvt-unicode-256color
URxvt*urgentOnBell:     false
URxvt*visualBell:       false
URxvt*urlLaucher:       firefox
URxvt*underlineURLs:    true
URxvt*urlButton:        1

! Appearance
URxvt*geometry:         100x30
URxvt*depth:            32
!URxvt*transparent:      true
URxvt*shading:          80
URxvt*underlineColor:   #4682B4
URxvt*highlightColor:   #666666
URxvt*throughColor:     #4682B4
URxvt*scrollBar:        false
URxvt*scrollTtyOutput:  false
URxvt*scrollWithBuffer: true
URxvt*scrollWithKeypress:   true
URxvt*internalBorder:   0
URxvt*externalBorder:   0
URxvt*colorIT:          #4682B4
URxvt*colorBD:          #F9F9F9
URxvt*colorUL:          #87AFD7

URxvt*iso14755:         False
URxvt*iso14755_52:      False

! Modified solarized colorscheme
! special
*.foreground:   #93a1a1
*.background:   #23252e
*.cursorColor:  #93a1a1

! black
*.color0:       #002b36
*.color8:       #657b83

! red
*.color1:       #dc322f
*.color9:       #dc322f

! green
*.color2:       #859900
*.color10:      #859900

! yellow
*.color3:       #b58900
*.color11:      #b58900

! blue
*.color4:       #268bd2
*.color12:      #268bd2

! magenta
*.color5:       #6c71c4
*.color13:      #6c71c4

! cyan
*.color6:       #2aa198
*.color14:      #2aa198

! white
*.color7:       #93a1a1
*.color15:      #fdf6e3

! Fonts
URxvt*font:             xft:Source Code Pro for Powerline:size=10
URxvt*imFont:           xft:Source Code Pro for Powerline:size=10

URxvt*imLocale: en_US.UTF-8

! Resize font: Ctrl+plus to increase, Ctrl+minus to decrease
URxvt.keysym.Control-0x2b:  font-size:increase
URxvt.keysym.Control-0x2d:  font-size:decrease

